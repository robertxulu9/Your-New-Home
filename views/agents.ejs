<%- include('partials/header') %>




<section class="ftco-section ftco-agent">
    <div class="container">
        <div class="row">
            <% agents.forEach(agent => { %>
            <div class="col-md-3 ftco-animate">
                <div class="agent">
                    <div class="img">
                        <img src="<%= agent.profilePicture || '/images/default-profile.jpg' %>" class="img-fluid" alt="<%= agent.name %>">
                    </div>
                    <div class="desc">
                        <h3><span  class="unclickable-link"href="/properties?agentId=<%= agent.id %>"><%= agent.name %></span></h3>
                        <p class="h-info"><span class="ion-ios-filing icon"></span> <span class="details"><%= agent.propertyCount %> Properties</span></p>
                    </div>
                </div>
            </div>
            <% }); %>
        </div>
